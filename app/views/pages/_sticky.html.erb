<%= link_to edit_sticky_path(sticky), :target => "_blank" do %>
  <div class="sticky_wrapper">
    <div class="sticky_wrapper sticky_header">
    </div>
    <div>
      <div class="sticky_wrapper content_container">
        <%= sticky.details %>
      </div>
      <div>
        <% sticky.owners.each do |owner| %>
          <%= render("member", :user => owner) %>
        <% end %>
      </div>
    </div>
  </div>
<% end %>